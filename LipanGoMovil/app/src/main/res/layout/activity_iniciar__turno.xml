<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_iniciar__turno"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="android.alberto.app.lipangomovil.Iniciar_Turno"
    app:layout_editor_absoluteX="0dp"
    app:layout_editor_absoluteY="80dp"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">
    <include layout="@layout/toolbar_inicio_turno"
        android:id="@+id/include2"
        android:layout_height="0dp"
        android:layout_width="0dp"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/getfecha"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="fecha"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/gethora"
        app:layout_constraintTop_toBottomOf="@+id/include2" />

    <TextView
        android:id="@+id/gethora"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="8dp"
        android:text="hora"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include2" />

    <TextView
        android:id="@+id/dataextra2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Nombre"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/getfecha" />

    <TextView
        android:id="@+id/instructivo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Descripcion"
        android:textSize="16sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dataextra2" />

    <CheckBox
        android:id="@+id/nombre1"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Abraham Estrada"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/instructivo"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/nombre8"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Rafael Flores"
        android:layout_marginRight="43dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/instructivo" />

    <CheckBox
        android:id="@+id/nombre3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Eugenio Perez"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre1"
        app:layout_constraintRight_toLeftOf="@+id/nombre4"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Francisco Báez"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre8"
        app:layout_constraintLeft_toLeftOf="@+id/nombre8"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre5"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Ignacio Cortés"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre3"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/nombre6"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Martín Cortés"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre4"
        app:layout_constraintLeft_toLeftOf="@+id/nombre4"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre9"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Rafael Medrano"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre5"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/nombre2"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp" />

    <CheckBox
        android:id="@+id/nombre2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Arturo Flores"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre6"
        android:layout_marginEnd="16dp" />

    <TextView
        android:id="@+id/trabajadores"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Trabajadores"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/button7"
        style="@android:style/Widget.Button.Inset"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:onClick="limpiar"
        android:text="Limpiar seleccion"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/nombre9"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button8"
        style="@android:style/Widget.Button.Inset"
        android:layout_width="161dp"
        android:layout_height="58dp"
        android:onClick="cancelar"
        android:text="Cancelar inicio de turno"
        app:layout_constraintTop_toTopOf="@+id/button9"
        android:layout_marginTop="0dp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button9"
        style="@android:style/Widget.Button.Inset"
        android:layout_width="168dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:onClick="registrar_turno"
        android:text="Confirmar inicio de turno"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/trabajadores" />

</android.support.constraint.ConstraintLayout>